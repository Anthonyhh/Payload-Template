{
  "name": "flowstateit-website-factory",
  "version": "1.0.0",
  "private": true,
  "engines": { "node": ">=20.10.0" },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "payload migrate && next start -p $PORT",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:unit": "vitest run --dir tests/unit",
    "test:int": "vitest run --dir tests/integration",
    "test:e2e": "playwright test",
    "test:load": "k6 run tests/load/k6-smoke.js",
    "bootstrap": "node scripts/bootstrap.mjs"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.58.0",
    "@payloadcms/payload-cloud": "^3.58.0",
    "@payloadcms/richtext-lexical": "^3.58.0",
    "@payloadcms/storage-r2": "^3.58.0",
    "@payloadcms/ui": "^3.58.0",
    "next": "^15.4.4",
    "payload": "^3.58.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "usesend-js": "^0.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.18.8",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.37.0",
    "playwright": "^1.54.1",
    "typescript": "^5.7.3",
    "vitest": "^3.2.3"
  }
}
